import cmath
p=complex(input().strip())
print(p)
p1=cmath.polar(p)
print(p1[0])
print(p1[1])


#without cmath
print("without using cmath module")
import math
p=input()
print(p)
print(p[0])
r=math.sqrt(pow(int(p[0]),2)+(pow(int(p[2]),2)))
print(r)
print(math.atan(int(p[2])/int(p[0])))

